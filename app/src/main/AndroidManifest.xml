<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.hp.main">
    <uses-sdk tools:overrideLibrary="com.firebase.ui,com.firebase.ui.auth,com.facebook,android.support.customtabs"



        />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="EverythingCollege"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:label">
        <activity
            android:name=".SplashScreen"
            android:noHistory="true"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".topHundredActivity"
            android:label="Top100"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".searchActivity"
            android:label="Search A College"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".examsActivity"
            android:label="Entrance Exams"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".eligibilityActivity"
            android:label="Eligiblity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".elgibilityAdapter" />
        <activity
            android:name=".scholarshipActivity"
            android:label="Scholarships"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".stateActivity" />
        <activity android:name=".states_intro" />
        <activity
            android:name=".SearchResult"
            android:parentActivityName=".MainActivity" />

        <activity android:name=".Signin"></activity>
    </application>

</manifest>